<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="StatusBadgeView">
        <attr name="icon" format="reference" />
        <attr name="foregroundTint" format="color" />
        <attr name="backgroundTint" format="color" />
    </declare-styleable>

    <declare-styleable name="ToolTitleView">
        <attr name="leftQuickActionIcon" format="reference" />
        <attr name="rightQuickActionIcon" format="reference" />
        <attr name="title" format="string" />
        <attr name="subtitle" format="string" />
        <attr name="showSubtitle" format="boolean" />
        <attr name="flattenQuickActions" format="boolean" />
    </declare-styleable>

</resources>